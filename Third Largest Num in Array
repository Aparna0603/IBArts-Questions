public class Main {
public static int thirdLargest(int arr[]) {
int result[] = removeDuplicates(arr);
Arrays.sort(result);
if(result.length<3) {
return -1;
}
return result[result.length-3];
}
public static int[] removeDuplicates(int arr[]) {
Set<Integer> set = new HashSet<>();
for(int num : arr) {
set.add(num)
}
int uniqueArr[] = new int[set.size()];
int index = 0;
for(int num: set) {
uniqueArr[index++] = num;
}
return result;
}
}
